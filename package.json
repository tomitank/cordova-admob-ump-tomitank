{
  "name": "cordova-admob-ump-tomitank",
  "version": "1.0.1",
  "description": "Google AdMob UMP SDK to Cordova.",
  "scripts": {
    "version": "sync-package-version --cordova-plugin"
  },
  "devDependencies": {
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "lintspaces-cli": "^0.7.1",
    "sync-package-version": "0.0.5"
  },
  "cordova": {
    "id": "cordova-admob-ump-tomitank",
    "platforms": [
      "android",
      "ios"
    ]
  },
  "repository": "https://github.com/tomitank/cordova-admob-ump-tomitank",
  "keywords": [
    "cordova",
    "ecosystem:cordova",
    "cordova-android",
    "cordova-ios",
    "ad",
    "admob",
    "google",
    "ump",
    "founding-choice",
    "founding",
    "choice"
  ],
  "author": "tomitank",
  "license": "MIT",
  "files": [
    "*.js",
    "*.md",
    "*.xml",
    "LICENSE",
    "src",
    "www"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "plugin.xml": [
      "lintspaces -d spaces -s 4"
    ]
  }
}
